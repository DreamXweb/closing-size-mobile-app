{
  "name": "size-match-mobile-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npx react-native run-android",
    "build_debug": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "start_expo": "expo start --dev-client",
    "android_expo": "expo run:android",
    "ios_expo": "expo run:ios",
    "web_expo": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.3.2",
    "expo": "~45.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-preference": "^2.2.3",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-select-dropdown": "^2.0.4",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-navigation/native": "^6.0.11"
  },
  "private": true
}
